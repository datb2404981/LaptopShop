    "bcrypt": "^6.0.0",
    "bootstrap": "^5.3.7",
    "bootstrap-icons": "^1.13.1",
    "connect-flash": "^0.1.1",
    "dotenv": "16.4.7",
    "ejs": "^3.1.10",
    "express": "^5.1.0",
    "express-ejs-layouts": "^2.5.1",
    "express-session": "1.18.2",
    "fuse.js": "^7.1.0",
    "module-alias": "^2.2.3",
    "multer": "1.4.5-lts.1",
    "mysql2": "^3.14.3",
    "passport": "0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-local": "^1.0.0",
    "uuid": "11.0.5",
    "zod": "4.1.5"
  },
  "name": "project_03",
  "version": "1.0.0",
  "description": "node js hoi_dan_it",
  "main": "server.ts",
  "devDependencies": {
    "@types/bcrypt": "6.0.0",
    "@types/connect-flash": "^0.0.40",
    "@types/express": "5.0.0",
    "@types/multer": "1.4.12",
    "@types/node": "22.10.7",
    "@types/passport": "1.0.17",
    "@types/passport-local": "^1.0.38",
    "cpx": "^1.5.0",
    "nodemon": "3.1.9",
    "prisma": "6.14.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3"
  },
  "scripts": {
    "build": "tsc && cpx \"src/views/**/*\" dist/views",
    "postinstall": "prisma generate",
    "start": "node -r module-alias/register dist/server.js",
    "dev": "nodemon -r ts-node/register src/server.ts",
    "start:debug": "nodemon --inspect=9229 -e ts,tsx --exec ts-node -r tsconfig-paths/register ./src/server.ts"
  },
  "_moduleAliases": {
    "config": "dist/config",
    "controllers": "dist/controllers",
    "models": "dist/models",
    "routers": "dist/routers",
    "services": "dist/services",
    "middleware": "dist/middleware",
    "validate": "dist/validate",
    "src": "dist"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts",
    "ignore": [
      "node_modules"
    ],
    "exec": "ts-node -r tsconfig-paths/register src/server.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/datb2404981/h-c-backend---hoidanit.git"
  },
  "keywords": [
